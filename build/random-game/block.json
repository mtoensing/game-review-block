{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "name": "game-review/random-game",
  "version": "4.7.0",
  "title": "Game Review Block - Random Game",
  "category": "widgets",
  "icon": "randomize",
  "supports": {
    "html": false,
    "align": true
  },
  "description": "Display a single random game.",
  "attributes": {
    "use_cache": {
      "type": "boolean",
      "default": false
    },
    "updated": {
      "type": "number",
      "default": 0
    },
    "min_rating": {
      "type": "number",
      "default": 1
    },
    "fontsize": {
      "type": "number"
    },
    "prefix": {
      "type": "string",
      "default": ""
    },
    "postfix": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "game-review-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}